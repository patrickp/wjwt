ACTION='SIGN' 
IF 0 THEN
    CALL WJWT.WOBJECT("CREATE",OPTIONS,"","",RERR)
    CALL WJWT.WOBJECT("ADD",OPTIONS,"key","secret",RERR)
    CALL WJWT.WOBJECT("CREATE",CLAIMS,"","",RERR)
    CALL WJWT.WOBJECT("ADD",CLAIMS,"Name","Patrick Payne",RERR)
    CALL WJWT.WOBJECT("ADD",OPTIONS,"expires_in","3600",RERR)
    CALL WJWT.WOBJECT("ADD",CLAIMS,"user_id","patrickp",RERR)
END
IF 1 THEN
    SECRET.KEY="secret"
    CALL WOBJ(OPTIONSOBJ,"FROMSTRING","","{}","",RERR)
    CALL WOBJ(OPTIONSOBJ,"SET","key",SECRET.KEY,"",RERR)
    CALL WOBJ(OPTIONSOBJ,"SET.NUMBER","expires_in","3600","",RERR)
    CALL WOBJ(CLAIMSOBJ,"FROMSTRING","","{}","",RERR)
    CALL WOBJ(CLAIMSOBJ,"SET.STRING","Name","Patrick Payne","",RERR)
    CALL WOBJ(CLAIMSOBJ,"SET.STRING","user_id","patrickp","",RERR)
END
CALL WJWT(ACTION,CLAIMSOBJ,PARAMOUT,OPTIONSOBJ,RERR)
JWT=PARAMOUT
PRINT "WEB TOKENT=":
PRINT JWT
CALL WJWT("VERIFY",JWT,PARAMOUT,OPTIONSOBJ,RERR)
PRINT "VERIFY=":PARAMOUT

